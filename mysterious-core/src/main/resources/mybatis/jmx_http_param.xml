<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lihuia.mysterious.core.mapper.jmx.sample.http.HttpParamMapper">
    <resultMap id="HttpParamResult" type="com.lihuia.mysterious.core.entity.jmx.sample.http.HttpParamDO">
        <result property="id" column="id"/>
        <result property="testCaseId" column="test_case_id"/>
        <result property="jmxId" column="jmx_id"/>
        <result property="httpId" column="http_id"/>
        <result property="paramKey" column="param_key"/>
        <result property="paramValue" column="param_value"/>
        <result property="creatorId" column="creator_id"/>
        <result property="creator" column="creator"/>
        <result property="modifierId" column="modifier_id"/>
        <result property="modifier" column="modifier"/>
        <result property="createTime" column="create_time"/>
        <result property="modifyTime" column="modify_time"/>
    </resultMap>

    <sql id="table_name">
        mysterious_jmx_http_param
    </sql>
    
    <sql id="select-column-list">
        `id`,
        `test_case_id`,
        `jmx_id`,
        `http_id`,
        `param_key`,
        `param_value`,
        `creator_id`,
        `creator`,
        `modifier_id`,
        `modifier`,
        `create_time`,
        `modify_time`
    </sql>
    
    <sql id="set">
        <if test="id != null">id=#{id},</if>
        <if test="testCaseId != null">test_case_id=#{testCaseId},</if>
        <if test="jmxId != null">jmx_id=#{jmxId},</if>
        <if test="httpId != null">http_id=#{httpId},</if>
        <if test="paramKey != null">param_key=#{paramKey},</if>
        <if test="paramValue != null">param_value=#{paramValue},</if>
        <if test="creatorId != null">creator_id=#{creatorId},</if>
        <if test="creator != null">creator=#{creator},</if>
        <if test="modifierId != null">modifier_id=#{modifierId},</if>
        <if test="modifier != null">modifier=#{modifier},</if>
        <if test="createTime != null">create_time=#{createTime},</if>
        <if test="modifyTime != null">modify_time=#{modifyTime},</if>
    </sql>

    <sql id="setBatch">
        <if test="item.id != null">id=#{item.id},</if>
        <if test="item.testCaseId != null">test_case_id=#{item.testCaseId},</if>
        <if test="item.jmxId != null">jmx_id=#{item.jmxId},</if>
        <if test="item.httpId != null">http_id=#{item.httpId},</if>
        <if test="item.paramKey != null">param_key=#{item.paramKey},</if>
        <if test="item.paramValue != null">param_value=#{item.paramValue},</if>
        <if test="item.creatorId != null">creator_id=#{item.creatorId},</if>
        <if test="item.creator != null">creator=#{item.creator},</if>
        <if test="item.modifierId != null">modifier_id=#{item.modifierId},</if>
        <if test="item.modifier != null">modifier=#{item.modifier},</if>
        <if test="item.createTime != null">create_time=#{item.createTime},</if>
        <if test="item.modifyTime != null">modify_time=#{item.modifyTime},</if>
    </sql>
    
    <insert id="add" parameterType="com.lihuia.mysterious.core.entity.jmx.sample.http.HttpParamDO">
        INSERT
        <include refid="table_name"/>
        <set>
            <include refid="set"></include>
        </set>
    </insert>

    <insert id="batchAdd" parameterType="list"> 
        <foreach collection="list" item="item" index="index" separator=";">
            INSERT
            <include refid="table_name"/>
            <set>
                <include refid="setBatch"></include>
            </set>
        </foreach>
    </insert>

    <select id="getListByHttpId" resultMap="HttpParamResult">
        SELECT
        <include refid="select-column-list"/>
        FROM
        <include refid="table_name"/>
        WHERE 1=1
        <if test="httpId != null">and http_id=#{httpId}</if>
    </select>

    <select id="getListByJmxId" resultMap="HttpParamResult">
        SELECT
        <include refid="select-column-list"/>
        FROM
        <include refid="table_name"/>
        WHERE 1=1
        <if test="jmxId != null">and jmx_id=#{jmxId}</if>
    </select>

    <select id="getExistParamList" resultMap="HttpParamResult">
        SELECT
        <include refid="select-column-list"/>
        FROM
        <include refid="table_name"/>
        WHERE 1=1
        <if test="httpId != null">and http_id=#{httpId}</if>
        <if test="paramKey != null">and param_key=#{paramKey}</if>
    </select>

    <update id="update" parameterType="com.lihuia.mysterious.core.entity.jmx.sample.http.HttpParamDO">
        UPDATE
        <include refid="table_name"/>
        <set>
            <include refid="set"></include>
        </set>
        WHERE 1=1
        <if test="id != null">and id=#{id}</if>
    </update>

    <update id="batchUpdate" parameterType="com.lihuia.mysterious.core.entity.jmx.sample.http.HttpParamDO">
        <foreach collection="list" item="item" index="index" separator=";">
            UPDATE
            <include refid="table_name"/>
            <set>
                <include refid="setBatch"></include>
            </set>
            WHERE 1=1
            <if test="item.id != null">and id=#{item.id}</if>
        </foreach>
    </update>

    <delete id="delete">
        DELETE FROM
        <include refid="table_name"/>
        WHERE 1=1
        <if test="id != null">and id=#{id}</if>
    </delete>

    <delete id="batchDelete">
        DELETE FROM
        <include refid="table_name"/>
        WHERE id in
        <foreach collection="list" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
